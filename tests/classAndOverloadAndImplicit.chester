// defines members in module A
class A {
  var a: Integer
  var b: Integer
}

module A {
  apply(a: Integer, b: Integer, ignored: Integer): A = A(a, b)
}

// [] means implicit. They are private fields?
class Box[a] {
  var first: a
}

B = Box
boxInt = B[Integer]


class Box2[a](first: a)

b2I = Box2(0)