// defines members in module A
class A {
    var a: Integer
    var b: Integer
}

module A {
    // A implicitly converts to A.type
    apply(a: Integer, b: Integer, ignored: Integer): A = A(a, b)
}


class Box[a] {
  var first: a
}

B = Box
// means Box.type(Integer)
boxInt = B[Integer]


class Box2[a](first: a)

b2I = Box2(0)